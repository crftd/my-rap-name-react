{
  "name": "my-rap-name-react",
  "version": "0.2.2",
  "private": true,
  "homepage": "/my-rap-name-react",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^3.9.4",
    "formik": "^1.5.8",
    "material-ui-pickers": "^2.2.4",
    "normalize.css": "^8.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "5.0.0",
    "styled-components": "^4.4.1",
    "yup": "^0.26.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "TZ=utc react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint ./src",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint": "yarn lint:css && yarn lint:js",
    "codeceptjs": "codeceptjs",
    "cypress": "cypress",
    "deploy:major": "yarn version --major && git push --follow-tags",
    "deploy:minor": "yarn version --minor && git push --follow-tags",
    "deploy:patch": "yarn version --patch && git push --follow-tags"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/serviceWorker.js",
      "!src/index.js"
    ]
  },
  "devDependencies": {
    "codeceptjs": "^2.6.11",
    "codeceptjs-resemblehelper": "^1.9.7",
    "cypress": "^3.8.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "eslint-config-airbnb-base": "^13.2.0",
    "husky": "^1.3.1",
    "jest-dom": "^3.5.0",
    "prettier": "1.16.4",
    "pretty-quick": "^1.11.1",
    "react-testing-library": "^6.1.2",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webdriverio": "^5.23.0"
  }
}
